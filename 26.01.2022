//Main.java

import java.io.*;
public class Main 
{
	


	public static void main(String[] args) 
	{
		punto A,B;
		A = new punto();
		B = new punto();
		prendi_punti(A, B);
		
		

	}
	public static void prendi_punti(punto A, punto B) 
	{
		InputStreamReader input = new InputStreamReader(System.in);
		BufferedReader tastiera= new BufferedReader (input);
		System.out.println("Inserire valori dei punti A e B");
		try 
		{
			System.out.println("Punto A");
			A.setPunto(Integer.valueOf(tastiera.readLine()), Integer.valueOf(tastiera.readLine()));
			System.out.println("Punto B");
			B.setPunto(Integer.valueOf(tastiera.readLine()), Integer.valueOf(tastiera.readLine()));
		}
		catch(Exception e) 
		{
			System.out.print("Errore input");
		}
	}

}

















//punto.java



public class punto 
{
	private int x;
	private int y;
	public punto() 
	{
		x=0;
		y=0;
	}
	public int getX()
	{
		return x;
	}
	public int getY()
	{
		return y;
	}
	public void setPunto(int xV, int yV) 
	{
		x=xV;
		y=yV;
	}
	
	public void traslaPunto(int dX, int dY) 
	{
		x=x+dX;
		y=y+dY;
	}
	
	public double distanza(punto p1, punto p2) 
	{
		return Math.sqrt((p2.getX()-p1.getX())*(p2.getX()-p1.getX())+(p2.getY()-p1.getY())*(p2.getY()-p1.getY()));
	}
	
	public String toString(int x, int y) 
	{
		String coord;
		coord=String.valueOf(x)+' '+String.valueOf(y); 
		return coord;
	}
	
}
